{
  "name": "litan",
  "displayName": "litan",
  "description": "Language support for Litan",
  "version": "0.0.1",
  "publisher": "Sera K. Litsch",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "litan",
      "aliases": ["Litan", "litan"],
      "extensions": [".ltn"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "litan",
      "scopeName": "source.ltn",
      "path": "./syntaxes/litan.tmLanguage.json"
    }],
    "snippets": [
      {
        "language": "litan",
        "path": "./snippets/snippets.code-snippets"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JeneLitsch/Litan-VSCode"
  }

}
